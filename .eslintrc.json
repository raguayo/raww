{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-await-in-loop": 0,
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": [
      0
    ],
    "react/require-default-props": [
      0
    ],
    "global-require": [
      0
    ],
    "func-names": 0,
    "no-param-reassign": 0,
    "semi": 1,
    "no-unused-vars": 1,
    "comma-dangle": 1,
    "max-len": 0,
    "linebreak-style": 0,
    "quotes": 0
  }
}
